{
  "rootGroups": [
    {
      "name": "global",
      "softMemoryLimit": "1MB",
      "hardConcurrencyLimit": 100,
      "maxQueued": 1000,
      "subGroups": [
        {
          "name": "morning",
          "softMemoryLimit": "2MB",
          "hardConcurrencyLimit": 2,
          "maxQueued": 3
        },
        {
          "name": "afternoon",
          "softMemoryLimit": "2MB",
          "hardConcurrencyLimit": 3,
          "maxQueued": 4
        },
        {
          "name": "evening",
          "softMemoryLimit": "2MB",
          "hardConcurrencyLimit": 3,
          "maxQueued": 4
        },
        {
          "name": "night",
          "softMemoryLimit": "2MB",
          "hardConcurrencyLimit": 3,
          "maxQueued": 4
        }
      ]
    }
  ],
  "selectors": [
    {
      "schedule" : "TZ=America/Los_Angeles; * * 0-11 ? * *",
      "group": "global.morning"
    },
    {
      "schedule" : "TZ=America/Los_Angeles; * * 12-16 ? * *",
      "group": "global.afternoon"
    },
    {
      "schedule" : "TZ=America/Los_Angeles; * * 17-19 ? * *",
      "group": "global.evening"
    },
    {
      "schedule" : "TZ=America/Los_Angeles; * * 20-23 ? * *",
      "group": "global.night"
    }
  ],
  "cpuQuotaPeriod": "1h"
}

